{
  "name": "owncloud_phoenix",
  "title": "ownCloud Phoenix",
  "icon": "folder",
  "version": "0.1.0",
  "description": "A safe home for all your data",
  "main": "core/core.bundle.js",
  "scripts": {
    "watch": "npm run build:dev && webpack-dev-server --progress --colors --watch --config webpack.dev.js --public $SERVER_HOST",
    "build:dev": "webpack -d --config webpack.dev.js",
    "build:prod": "webpack -p --config webpack.prod.js",
    "lint": "eslint src/**/*.vue src/**/*.js --color --global requirejs --global require",
    "accessibility-tests": "node node_modules/axe-cli/axe-cli --disable page-has-heading-one,meta-viewport",
    "accessibility-tests-drone": "node node_modules/axe-cli/axe-cli --exit --disable page-has-heading-one,meta-viewport",
    "acceptance-tests": "cucumber-js --require-module babel-core/register --require tests/acceptance/setup-local.js --require tests/acceptance/stepDefinitions --format node_modules/cucumber-pretty tests/acceptance/features",
    "acceptance-tests-drone": "cucumber-js --require-module babel-core/register --require tests/acceptance/setup-drone.js --require tests/acceptance/stepDefinitions --format node_modules/cucumber-pretty tests/acceptance/features"
  },
  "author": "Felix Heidecke",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/owncloud/phoenix",
  "devDependencies": {
    "axe-cli": "^3.0.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "chromedriver": "^2.42.0",
    "css-loader": "^0.28.11",
    "cucumber": "^5.0.2",
    "cucumber-pretty": "^1.4.0",
    "easygettext": "^2.7.0",
    "less": "^3.8.1",
    "nightwatch": "^1.0.11",
    "nightwatch-api": "^0.4.0",
    "start-server-and-test": "^1.7.4",
    "vue-loader": "14.2.3",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "js-owncloud-client": "^0.1.3",
    "lodash": "^4.17.11",
    "requirejs": "^2.3.6",
    "uikit": "^3.0.0-rc.9",
    "vue": "^2.5.16",
    "vue-drag-drop": "^1.1.4",
    "vue-events": "^3.1.0",
    "vue-gettext": "^2.1.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  }
}
